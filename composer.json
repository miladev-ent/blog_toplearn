{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "barryvdh/laravel-debugbar": "^3.6",
        "cyrildewit/eloquent-viewable": "dev-master",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "milwad/laravel-validate": "^1.6",
        "morilog/jalali": "3.*",
        "opcodesio/log-viewer": "^1.7",
        "overtrue/laravel-like": "^5.0",
        "realrashid/sweet-alert": "^6.0",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Mlk\\Home\\": "Modules/Mlk/Home/",
            "Mlk\\User\\": "Modules/Mlk/User/",
            "Mlk\\Auth\\": "Modules/Mlk/Auth/",
            "Mlk\\Panel\\": "Modules/Mlk/Panel/",
            "Mlk\\Category\\": "Modules/Mlk/Category/",
            "Mlk\\Role\\": "Modules/Mlk/Role/",
            "Mlk\\Article\\": "Modules/Mlk/Article/",
            "Mlk\\Comment\\": "Modules/Mlk/Comment/",
            "Mlk\\Share\\": "Modules/Mlk/Share/",
            "Mlk\\Advertising\\": "Modules/Mlk/Advertising/"
        },
        "files": [
            "Modules/Mlk/Share/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
